<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <applyto>/apps/peacock/author/name</applyto>
      <key>/schemas/apps/peacock/author/name</key>
      <owner>peacock</owner>
      <type>string</type>
      <locale name="C">
        <short>Author's Full Name</short>
        <long>The Full Name of the web page's author. This is used
when inserting the name of the author in the HTML file
using meta tags.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/author/email</applyto>
      <key>/schemas/apps/peacock/author/email</key>
      <owner>peacock</owner>
      <type>string</type>
      <locale name="C">
        <short>Author's email address</short>
        <long>The full email address of the web page's author. This is
used when inserting the author's email address in the
HTML using meta tags.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/html-toolbars/basic</applyto>
      <key>/schemas/apps/peacock/editor/html-toolbars/basic</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show the basic HTML Toolbars.</short>
        <long>Wether to display the Basic HTML tags toolbar.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/html-toolbars/fonts</applyto>
      <key>/schemas/apps/peacock/editor/html-toolbars/fonts</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short/>
        <long>Wether to display the Font related tags toolbar.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/html-toolbars/headings</applyto>
      <key>/schemas/apps/peacock/editor/html-toolbars/headings</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short/>
        <long>Wether to display the Headings related tags toolbar.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/html-toolbars/lists</applyto>
      <key>/schemas/apps/peacock/editor/html-toolbars/lists</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short/>
        <long>Wether to display the Lists related tags toolbar.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/html-toolbars/forms</applyto>
      <key>/schemas/apps/peacock/editor/html-toolbars/forms</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short/>
        <long>Wether to display the Forms related tag toolbar.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/html-toolbars/tables</applyto>
      <key>/schemas/apps/peacock/editor/html-toolbars/tables</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short/>
        <long>Wether to display the Tables related tags toolbar.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/html/meta/author-info</applyto>
      <key>/schemas/apps/peacock/html/meta/author-info</key>
      <owner>peacock</owner>
      <type>bool</type>
      <locale name="C">
        <short/>
        <long>Wether to insert author's info in the HTML using meta tags.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/html/meta/generator-info</applyto>
      <key>/schemas/apps/peacock/html/meta/generator-info</key>
      <owner>peacock</owner>
      <type>bool</type>
      <locale name="C">
        <short/>
        <long>Wether to insert Generator's info in the HTML using meta
 tags.</long>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/toolbars/main</applyto>
      <key>/schemas/apps/peacock/editor/toolbars/main</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show/Hide the main toolbar.</short>
        <long/>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/toolbars/html</applyto>
      <key>/schemas/apps/peacock/editor/toolbars/html</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show/Hide the html toolbars.</short>
        <long/>
      </locale>
    </schema>
    <schema>
      <applyto>/apps/peacock/editor/toolbars/file-browser</applyto>
      <key>/schemas/apps/peacock/editor/toolbars/file-browser</key>
      <owner>peacock</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show/Hide the file-browser.</short>
        <long/>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
